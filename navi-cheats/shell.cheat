% shell

# delete broken symlinks
find . -maxdepth 1 -xtype l -delete

# find files edited within the last day
find . -maxdepth 3 \
         -name node_modules -prune -o \
         -name .git -prune -o \
         -type f -mtime -1 -exec ls -gol --time-style=iso {} \; | sed 's|^[^ ]\+\? . [0-9]\+ ||'

